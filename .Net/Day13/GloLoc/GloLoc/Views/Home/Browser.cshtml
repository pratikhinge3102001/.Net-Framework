@{
    ViewData["Title"] = "Browser Page";
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>

<table class="table culture-table">
    <tr>
        <td>Date</td>
        <td>
            @DateTime.Now.ToLongDateString()
        </td>
    </tr>
    <tr>
        <td>Currency</td>
        <td>
            @(12345.00.ToString("c"))
        </td>
    </tr>
    <tr>
        <td>Number</td>
        <td>
            @(123.45m.ToString("F2"))
        </td>
    </tr>
</table>